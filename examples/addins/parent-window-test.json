        "parent.html": "<!DOCTYPE html><html><head><meta charset='UTF-8'><title>Parent Window Test</title></head><body><h1>Testing Parent Window Access</h1><div id='log'></div><script>var log=document.getElementById('log');function addLog(msg){console.log(msg);log.innerHTML+='<p>'+msg+'</p>';}addLog('Page loaded');addLog('window.parent: '+(window.parent?'EXISTS':'NULL'));addLog('window.top: '+(window.top?'EXISTS':'NULL'));if(window.parent&&window.parent.geotab){addLog('window.parent.geotab: EXISTS');addLog('Keys: '+Object.keys(window.parent.geotab).join(', '));}else{addLog('window.parent.geotab: NULL');}if(window.top&&window.top.geotab){addLog('window.top.geotab: EXISTS');addLog('Keys: '+Object.keys(window.top.geotab).join(', '));}else{addLog('window.top.geotab: NULL');}setTimeout(function(){addLog('Checking after 1 second...');if(window.parent&&window.parent.api){addLog('window.parent.api: EXISTS');}if(window.top&&window.top.api){addLog('window.top.api: EXISTS');}},1000);</script></body></html>",
        "translations": {
            "en.json": "{\"name\":\"Parent Window Test\"}"
        }
    }
