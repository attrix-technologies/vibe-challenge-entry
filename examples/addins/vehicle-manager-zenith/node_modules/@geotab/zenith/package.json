{
  "name": "@geotab/zenith",
  "version": "1.26.6",
  "description": "Zenith components library on React",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": [
    "**/*.css",
    "**/*.less",
    "**/react-chartjs/dateAdapter.ts",
    "**/react-chartjs/dateAdapter.js"
  ],
  "scripts": {
    "test": "npm run clean && npm run test-build && jest",
    "start": "npm run storybook --loglevel verbose",
    "lint": "npx eslint -c \".eslintrc.js\" --ext \".ts\" --ext \".tsx\" \"**/*.tsx\" \"**/*.ts\" --ignore-path \".eslintignore\"",
    "new": "node newComponent.mjs",
    "build-ai-mdx": "node collectmdx.js",
    "build-esm": "npx tsc -b tsconfig.esm.json",
    "build-cjs": "npx tsc -b tsconfig.cjs.json",
    "build-ts": "npm run build-esm && npm run build-cjs && node less-transformer.js",
    "build-less": "node less-compile.js",
    "build-copy": "node build-copy.js",
    "build-clean": "node build-clean.js",
    "build-index": "node buildIndex.js",
    "update-version": "node updateVersion.js",
    "build": "npm run build-index && npm run update-version && npm run build-ts && npm run build-less && npm run build-copy && npm run build-clean",
    "add-icon": "node addIcon.js",
    "watch": "npx tsc -b tsconfig.build.json -w",
    "test-build": "npx tsc -b tsconfig.jestTests.json",
    "test-watch": "npm run clean && npx tsc -b tsconfig.jestTests.json -w",
    "test-coverage": "npm run clean && npm run test-build && jest --collectCoverage=true --coverage --collectCoverageFrom=./**/*.js --json --outputFile=jest-result.json --coverageReporters=html --coverageReporters=text-summary --coverageReporters=cobertura --maxWorkers=4 --noStackTrace && node testHelpers/cobertura/cobertura.js",
    "coverage": "npm run test-coverage && google-chrome ./jest-coverage/index.html",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build && npm run build-ai-mdx",
    "clean": "rm -rf ./dist && rm -rf ./esm && rm -rf ./jest-coverage"
  },
  "author": "",
  "license": "SEE LICENSE IN LICENSE.txt",
  "devDependencies": {
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@storybook/addon-docs": "^9.1.15",
    "@storybook/addon-links": "^9.1.15",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.6",
    "@storybook/react-webpack5": "^9.1.15",
    "@tanstack/react-query": "^5.85.5",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "axe-core": "^4.10.2",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^9.1.15",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-junit": "^16.0.0",
    "less": "^4.1.3",
    "less-loader": "^12.2.0",
    "mini-css-extract-plugin": "^2.9.2",
    "storybook": "^9.1.15",
    "style-loader": "^4.0.0",
    "typescript": "^5.4.3",
    "webpack": "^5.96.1",
    "xml2js": "^0.6.0"
  },
  "dependencies": {
    "chart.js": "^4.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "chart.js": "^4.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "files": [
    "./dist/"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
