        "pattern.html": "<!DOCTYPE html><html><head><meta charset='UTF-8'><title>Pattern Test</title></head><body><h1>Testing geotab.addin pattern</h1><div id='log'></div><script>var log=document.getElementById('log');function addLog(msg){console.log(msg);log.innerHTML+='<p>'+msg+'</p>';}addLog('Page loaded');if(typeof geotab==='undefined'){window.geotab={};}if(typeof geotab.addin==='undefined'){geotab.addin={};}geotab.addin.objectPattern=function(){var api;return{initialize:function(freshApi,state,callback){addLog('initialize called via geotab.addin!');api=freshApi;addLog('API: '+(api?'EXISTS':'NULL'));if(api&&api.getSession){api.getSession(function(cred){addLog('Got session: '+JSON.stringify(cred));},function(err){addLog('Session error: '+JSON.stringify(err));});}callback();},focus:function(freshApi,state){addLog('focus called');},blur:function(freshApi,state){addLog('blur called');}};}();addLog('geotab.addin.objectPattern registered');</script></body></html>",
